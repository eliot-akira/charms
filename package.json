  "scripts": {
    "dev": "babel src --out-dir build --extensions \".js,.ts,.tsx\" --watch & sleep 3 && nodemon --watch build build/test",
    "build": "babel src --out-dir build --extensions \".js,.ts,.tsx\" && tsc --declaration --emitDeclarationOnly",
    "test": "yarn build && node build/test",
    "type": "tsc --noEmit",
    "clean": "rm -rf build",
    "release": "yarn build && cp package.json build && cp readme.md build && cd build"
  },
