    "nedb": "^1.8.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.6",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.7.16",
